{
    "license": "proprietary",
    "type": "project",
    "require": {
        "php": ">=8.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "baldinof/roadrunner-bundle": "^3.2.2",
        "doctrine/dbal": "^4.2.3",
        "doctrine/doctrine-fixtures-bundle": "^4.1.0",
        "doctrine/doctrine-migrations-bundle": "^3.4.1",
        "doctrine/orm": "^3.3.2",
        "doctrine/persistence": "^4.0",
        "ramsey/uuid": "^4.7.6",
        "symfony/console": "~7.2.5",
        "symfony/dotenv": "~7.2.0",
        "symfony/flex": "^2.5.0",
        "symfony/framework-bundle": "~7.2.5",
        "symfony/http-kernel": "~7.2.5",
        "symfony/messenger": "~7.2.5",
        "symfony/runtime": "~7.2.3",
        "symfony/serializer": "~7.2.5",
        "symfony/yaml": "~7.2.5"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.45",
        "friendsofphp/php-cs-fixer": "^3.75",
        "icanhazstring/composer-unused": "^0.9.2",
        "maglnet/composer-require-checker": "^4.16.1",
        "phpstan/phpstan": "^2.1.11",
        "phpunit/phpunit": "^12.1.2",
        "rector/rector": "^2.0.11"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "MovieRate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MovieRate\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.2.*"
        }
    }
}
